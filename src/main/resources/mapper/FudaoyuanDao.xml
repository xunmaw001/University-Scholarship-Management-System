        <result property="fudaoyuangonghao" column="fudaoyuangonghao"/>
        <result property="fudaoyuanxingming" column="fudaoyuanxingming"/>
        <result property="mima" column="mima"/>
        <result property="xingbie" column="xingbie"/>
